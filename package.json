{
  "name": "plirono-api",
  "description": "An API and webserver that expose the Plirono database to other applications and the world.",
  "version": "0.0.0",
  "author": "Plirono",
  "homepage": "http://api.plirono.com/",
  "private": true,
  "scripts": {
    "postinstall": "scripts/postinstall.sh",
    "start": "bin/www",
    "watch": "grunt dev",
    "build": "grunt build",
    "localmongo": "mkdir -p mongo-test && mongod --dbpath mongo-test --port 37017",
    "test": "grunt test",
    "release": "scripts/release.sh",
    "clean": "grunt clean",
    "purge": "git clean -fdx"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "dotenv": "^2.0.0",
    "errorhandler": "~1.5.0",
    "express": "~4.14.0",
    "method-override": "~2.3.7",
    "mongoose": "~4.7.4",
    "morgan": "~1.7.0",
    "q": "~1.4.1"
  },
  "devDependencies": {
    "@types/body-parser": "latest",
    "@types/chai": "latest",
    "@types/cookie-parser": "latest",
    "@types/errorhandler": "latest",
    "@types/express": "latest",
    "@types/method-override": "latest",
    "@types/mocha": "latest",
    "@types/mongodb": "latest",
    "@types/mongoose": "latest",
    "@types/morgan": "latest",
    "angular-precommit": "~1.0.3",
    "chai": "~3.5.0",
    "grunt": "~1.0.1",
    "grunt-concurrent": "~2.3.1",
    "grunt-contrib-clean": "~1.0.0",
    "grunt-contrib-copy": "~1.0.0",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-mocha-test": "~0.13.2",
    "grunt-nodemon": "~0.4.2",
    "grunt-ts": "~6.0.0-beta.3",
    "grunt-tslint": "~4.0.0",
    "mocha": "~3.2.0",
    "mocha-typescript": "~1.0.15",
    "standard-version": "~4.0.0",
    "tslint": "~4.2.0",
    "tslint-eslint-rules": "~3.2.0",
    "typescript": "~2.1.4"
  },
  "main": "bin/www",
  "keywords": [
    "plirono",
    "api"
  ]
}
