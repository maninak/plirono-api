{
  "name": "plirono-api",
  "description": "An API and webserver that expose the Plirono database to other applications and the world.",
  "version": "0.0.0",
  "author": "Plirono",
  "homepage": "http://api.plirono.com/",
  "private": true,
  "scripts": {
    "postinstall": "scripts/postinstall.sh",
    "start": "grunt && NODE_ENV=production node ./bin/www",
    "watch": "grunt && NODE_ENV=development nodemon ./bin/www",
    "build": "rm -rf dist && grunt",
    "test": "rm -rf dist/test && grunt && mocha dist/test",
    "release": "scripts/release.sh",
    "clean": "git clean -fdX"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "errorhandler": "~1.5.0",
    "express": "~4.14.0",
    "method-override": "~2.3.7",
    "mongoose": "~4.7.4",
    "morgan": "~1.7.0",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "@types/body-parser": "~0.0.33",
    "@types/chai": "^3.4.34",
    "@types/cookie-parser": "~1.3.30",
    "@types/errorhandler": "~0.0.30",
    "@types/express": "~4.0.34",
    "@types/method-override": "~0.0.29",
    "@types/mocha": "^2.2.34",
    "@types/mongodb": "~2.1.36",
    "@types/mongoose": "~4.7.1",
    "@types/morgan": "~1.7.32",
    "angular-precommit": "~1.0.3",
    "chai": "^3.5.0",
    "grunt": "~1.0.1",
    "grunt-contrib-copy": "~1.0.0",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-ts": "~6.0.0-beta.3",
    "mocha": "^3.2.0",
    "mocha-typescript": "^1.0.15",
    "nodemon": "~1.11.0",
    "standard-version": "~4.0.0",
    "tslint-eslint-rules": "~3.2.0",
    "typescript": "~2.1.4"
  },
  "keywords": [
    "plirono",
    "api"
  ]
}
